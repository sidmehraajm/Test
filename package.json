{
  "name": "gitbook-plugin-search",
  "description": "Search input for your GitBook",
  "main": "index.js",
  "version": "2.2.1",
  "engines": {
    "gitbook": ">=3.0.0-pre.0"
  },
  "dependencies": {
    "gitbook-plugin-ga": "^2.0.0",
    "gitbook-plugin-insert-logo": "^0.1.5"
  },
  "homepage": "https://github.com/GitbookIO/plugin-search",
  "repository": {
    "type": "git",
    "url": "https://github.com/GitbookIO/plugin-search.git"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/GitbookIO/plugin-search/issues"
  },
  "devDependencies": {
    "eslint": "^2.7.0",
    "less": "2.5.1"
  },
  "scripts": {
    "prepublish": "lessc ./less/search.less > ./assets/search.css"
  },
  "plugins": [
		"comments-footer"
	],
	"pluginsConfig": {
		"comments-footer": {
			"type": "github",
			"repo": "introduction-to-rigging/what-is-rigging",
		}
	}
}
